    "prepublishOnly": "npm run build && npm run site"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-watch": "^7.0.0",
    "graphology-types": "^0.19.2",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.21.2",
    "typescript": "^4.2.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
